<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.nike.llow.domain">

    <class name="Comment" table="tb_comment" schema="db_llow">
        <id name="commentId" column="comment_id">
        	<generator class="native"></generator>
        </id>
        <property name="userId" column="user_id"/>
        <property name="productId" column="product_id"/>
        <property name="starCount" column="star_count"/>
        <property name="commentConte" column="comment_conte"/>
        <property name="commentTime" column="comment_time"/>
        <property name="standardName" column="standard_name"/>
    </class>
</hibernate-mapping>