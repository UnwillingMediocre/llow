<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.nike.llow.domain">

    <class name="Standard" table="tb_standard" schema="db_llow">
        <id name="standardId" column="standard_id">
        	<generator class="native"></generator>
        </id>
        <property name="productId" column="product_id"/>
        <property name="standardName" column="standard_name"/>
        <property name="price" column="price"/>
        <property name="stock" column="stock"/>
    </class>
</hibernate-mapping>