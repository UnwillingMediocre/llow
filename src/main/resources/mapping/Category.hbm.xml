<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.nike.llow.domain">

    <class name="Category" table="tb_category" schema="db_llow">
        <id name="categoryId" column="category_id">
        	<generator class="native"></generator>
        </id>
        <property name="categoryName" column="category_name"/>
        <property name="categoryStatus" column="category_status"/>
    </class>
</hibernate-mapping>